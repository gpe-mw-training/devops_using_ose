{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "annotations": {
                    "description": "The web server's http port."
                },
                "labels": {
                    "application": "srep",
                    "template": "eap6-basic-sti"
                },
                "name": "srep"
            },
            "spec": {
                "ports": [
                    {
                        "port": 8080,
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "deploymentConfig": "srep"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "annotations": {
                    "description": "Ping service for clustered applications."
                },
                "labels": {
                    "application": "srep",
                    "template": "eap6-basic-sti"
                },
                "name": "srep-ping"
            },
            "spec": {
                "portalIP": "None",
                "ports": [
                    {
                        "port": 8888,
                        "targetPort": 8888
                    }
                ],
                "selector": {
                    "deploymentConfig": "srep"
                }
            }
        },
        {
            "apiVersion": "v1",
            "id": "srep-http-route",
            "kind": "Route",
            "metadata": {
                "annotations": {
                    "description": "Route for application's http service."
                },
                "labels": {
                    "application": "srep",
                    "template": "eap6-basic-sti"
                },
                "name": "srep-http-route"
            },
            "spec": {
                "host": "",
                "to": {
                    "name": "srep"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "ImageStream",
            "metadata": {
                "labels": {
                    "application": "srep",
                    "template": "eap6-basic-sti"
                },
                "name": "srep"
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "srep",
                "labels": {
                    "app": "srep",
                    "template": "eap6-basic-sti"
                }
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "srep:latest"
                    }
                },
                "source": {
                    "contextDir": "",
                    "git": {
                        "ref": "master",
                        "uri": "https://github.com/gpe-mw-training/session-replication"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "jboss-eap64-openshift:1.4",
                            "namespace": "openshift"
                        },
			"env": [
      			  {
        			"name": "MAVEN_ARGS",
        			"value": "clean package -DskipTests -s maven/nexus-hardcoded-settings.xml"
      			  }
    			]
                    },
                    "type": "Source"
                },
                "triggers": [
                    {
                        "github": {
                            "secret": "RiaxUMKv"
                        },
                        "type": "GitHub"
                    },
                    {
                        "generic": {
                            "secret": "rBRvI8v6"
                        },
                        "type": "Generic"
                    },
                    {
                        "imageChange": {},
                        "type": "ImageChange"
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "DeploymentConfig",
            "metadata": {
                "labels": {
                    "application": "srep",
                    "template": "eap6-basic-sti"
                },
                "name": "srep"
            },
            "spec": {
                "replicas": 0,
                "selector": {
                    "deploymentConfig": "srep"
                },
                "strategy": {
                    "type": "Recreate"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "application": "srep",
                            "deploymentConfig": "srep"
                        },
                        "name": "srep"
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [
                                    {
                                        "name": "OPENSHIFT_KUBE_PING_NAMESPACE",
                                        "valueFrom": { "fieldRef": { "fieldPath": "metadata.namespace" }}
                                    },
                                    {
                                        "name": "OPENSHIFT_DNS_PING_SERVICE_NAME",
                                        "value": "srep-ping"
                                    },
                                    {
                                        "name": "OPENSHIFT_DNS_PING_SERVICE_PORT",
                                        "value": "8888"
                                    },
                                    {
                                        "name": "HORNETQ_CLUSTER_PASSWORD",
                                        "value": "SuMnXIWS"
                                    },
                                    {
                                        "name": "HORNETQ_QUEUES",
                                        "value": ""
                                    },
                                    {
                                        "name": "HORNETQ_TOPICS",
                                        "value": ""
                                    }
                                ],
                                "image": "srep",
                                "imagePullPolicy": "Always",
                                "name": "srep",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "name": "http",
                                        "protocol": "TCP"
                                    },
                                    {
                                        "containerPort": 8888,
                                        "name": "ping",
                                        "protocol": "TCP"
                                    }
                                ],
                                "readinessProbe": {
                                    "exec": {
                                        "command": [
                                            "/bin/bash",
                                            "-c",
                                            "/opt/eap/bin/readinessProbe.sh"
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                },
                "triggers": [
                    {
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "srep"
                            ],
                            "from": {
                                "kind": "ImageStream",
                                "name": "srep"
                            }
                        },
                        "type": "ImageChange"
                    }
                ]
            }
        }
    ]
}
